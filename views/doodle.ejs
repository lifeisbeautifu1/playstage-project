<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/public/css/doodle.css"></link>
    <title>Doodle Jump</title>
  </head>

  <body>

    
    <div class="grid"></div>

    <div id="my-modal" class="modal">

      <div class="modal-content">

          <div class="modal-header">

              <h2>Game Over</h2>
          
          </div>

          <div class="modal-body">

              <p id="text"></p>

              <button id="modal-btn" class="button">Play again</button>

              <a href="/dashboard" class="button ml-5">Go back</a>

          </div>

          <div class="modal-footer">

              <h3>Doodle Jump</h3>

          </div>

      </div>

    </div>
    <script src="/public/js/doodle.js"></script>
    <script src="/public/js/timespend.js"></script>
  </body>
</html>